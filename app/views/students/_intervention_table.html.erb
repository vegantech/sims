<% if !interventions.blank? %>
  <table class="intervention_list">
    <col style="width: 17%;" />
    <col style="width: 25.5%;" />
    <col style="width: 12%;" />
    <col style="width: 25.5%;" />
    <col style="width: 20%;" />
    <thead>
      <tr>
        <th>Objective Title</th>
        <th>Intervention Title</th>
        <th>Start Date</th>
        <th>Progress Monitor</th>
        <th> </th> 
      </tr>
    </thead>
    <tbody>
      <%=render :partial => 'students/intervention', :collection => interventions, :locals=>{:no_action=>(defined?no_action && no_action)} %>
    </tbody>
  </table>
<% end %>

