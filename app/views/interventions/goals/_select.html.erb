<div class="form_row">
<% if goals.blank? %>
  Interventions are not available, no goals defined.
  <%= link_to :back, :back %>
<%else%>
  <%= form_tag interventions_goals_path(:custom_intervention => custom_intervention?), :remote => true do%>
    <%= label_tag(:goal_id,'Assign Goal') %>
    <%= goal_select %>
<%= spinner("goals") %>
      <noscript><%= submit_tag("Choose Goal") %></noscript>
  <% end %>
<% end %>
</div>
<%= render :partial => 'interventions/objectives/select' %>
