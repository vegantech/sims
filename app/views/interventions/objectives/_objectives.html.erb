<div id="objectives" class="form_row">
<% unless objectives.blank? %>
  <%= form_tag new_path,
    :remote => true do %>
      <%= label_tag(:objective_id, 'Assign Objective') %>
      <%= intervention_picker_select(objectives) %>
    <%= spinner("objectives") %>
    <%= content_tag :noscript,  submit_tag("Choose Objective") %>
  <% end %>
<% end %>
</div>
<%= render objectives.categories %>
