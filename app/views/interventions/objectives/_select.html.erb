<div id="objectives" class="form_row">
<% if objectives %>
  <%= form_tag interventions_objectives_path(@goal_definition,
    :custom_intervention => custom_intervention?),
    :remote => true do %>
      <%= label_tag(:objective_id, 'Assign Objective') %>
      <%= objective_select %>
    <%= spinner("objectives") %>
    <%= content_tag :noscript,  submit_tag("Choose Objective") %>
  <% end %>
<% end %>
</div>
<%= render :partial=>"interventions/categories/select" %>
